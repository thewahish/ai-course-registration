{
  "name": "ai-course-registration",
  "version": "2.0.0",
  "description": "Comprehensive AI Course Registration & Reseller Management System - Arabic Language Support",
  "main": "index.html",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "webpack --mode production",
    "lint": "eslint . --ext .js,.html",
    "lint:fix": "eslint . --ext .js,.html --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/obaisukar/ai-course-registration.git"
  },
  "keywords": [
    "ai-course",
    "arabic-education",
    "reseller-management", 
    "payment-processing",
    "course-registration",
    "commission-tracking",
    "stripe-integration",
    "zoho-crm"
  ],
  "author": {
    "name": "Obai Sukar",
    "email": "obai@obaisukar.com",
    "url": "https://obaisukar.com"
  },
  "license": "Proprietary",
  "private": true,
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "stripe": "^14.12.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "nodemailer": "^6.9.7",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.1",
    "csv-parser": "^3.0.0",
    "multer": "^1.4.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "supertest": "^6.3.3",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "babel": {
    "presets": ["@babel/preset-env"]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}